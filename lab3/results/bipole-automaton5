	q3
	q0
	q4
	q1 [shape=doublecircle]
	q5 [shape=doublecircle]
	q2
	q0 -> q1 [label=a]
	q0 -> q2 [label=b]
	q1 -> q3 [label=a]
	q1 -> q4 [label=b]
	q2 -> q5 [label=a]
	q2 -> q3 [label=b]
	q3 -> q2 [label=a]
	q3 -> q1 [label=b]
	q4 -> q0 [label=a]
	q4 -> q5 [label=b]
	q5 -> q4 [label=a]
	q5 -> q0 [label=b]
	q0 [fillcolor=green style=filled]
	q1 [fillcolor=red style=filled]
	q5 [fillcolor=red style=filled]
}
