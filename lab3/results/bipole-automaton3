// Bipole
digraph {
	q1 [shape=doublecircle]
	q3 [shape=doublecircle]
	q0
	q2
	q0 -> q1 [label=a]
	q0 -> q2 [label=b]
	q1 -> q3 [label=a]
	q1 -> q0 [label=b]
	q2 -> q0 [label=a]
	q2 -> q3 [label=b]
	q3 -> q2 [label=a]
	q3 -> q1 [label=b]
	q0 [fillcolor=green style=filled]
	q1 [fillcolor=red style=filled]
	q3 [fillcolor=red style=filled]
}
